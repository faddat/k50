{
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^16.0.0",
    "htmlhint": "^1.1.4",
    "postcss": "^8.5.3",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "stylelint": "^16.18.0",
    "stylelint-config-standard": "^38.0.0"
  },
  "name": "k50",
  "version": "1.0.0",
  "description": "Interactive graph visualization with music reactivity",
  "main": "index.js",
  "dependencies": {
    "ajv": "^8.17.1",
    "ansi-styles": "^6.2.1",
    "balanced-match": "^3.0.1",
    "brace-expansion": "^4.0.0",
    "callsites": "^4.2.0",
    "chalk": "^5.4.1",
    "color-convert": "^3.0.1",
    "color-name": "^2.0.0",
    "concat-map": "^0.0.2",
    "escape-string-regexp": "^5.0.0",
    "fast-levenshtein": "^3.0.0",
    "file-entry-cache": "^10.0.8",
    "find-up": "^7.0.0",
    "flat-cache": "^6.1.8",
    "has-flag": "^5.0.1",
    "ignore": "^7.0.3",
    "isexe": "^3.1.1",
    "json-schema-traverse": "^1.0.0",
    "keyv": "^5.3.2",
    "locate-path": "^7.2.0",
    "minimatch": "^10.0.1",
    "p-limit": "^6.2.0",
    "p-locate": "^6.0.0",
    "parent-module": "^3.1.0",
    "path-exists": "^5.0.0",
    "path-key": "^4.0.0",
    "resolve-from": "^5.0.0",
    "shebang-regex": "^4.0.0",
    "stats.js": "^0.17.0",
    "strip-json-comments": "^5.0.1",
    "supports-color": "^10.0.0",
    "three": "^0.175.0",
    "which": "^5.0.0",
    "yocto-queue": "^1.2.1"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md,html}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md,html}\"",
    "lint": "npm run format && npm run lint:js:fix && npm run lint:css:fix && npm run lint:html",
    "lint:js": "eslint --ignore-path .eslintignore .",
    "lint:js:fix": "eslint --ignore-path .eslintignore --fix .",
    "lint:css": "stylelint --ignore-path .stylelintignore \"**/*.{css,scss}\"",
    "lint:css:fix": "stylelint --ignore-path .stylelintignore --fix \"**/*.{css,scss}\"",
    "lint:html": "htmlhint --config .htmlhintrc \"**/*.{html,htm}\"",
    "validate": "npm run format:check && npm run lint:js && npm run lint:css && npm run lint:html",
    "clean": "rm -rf public",
    "create-dirs": "mkdir -p public/js/lib public/js/modules",
    "copy-static": "cp -r js/modules/* public/js/modules/",
    "copy-deps": "cp node_modules/three/build/three.module.js public/js/lib/ && cp node_modules/stats.js/build/stats.min.js public/js/lib/",
    "build": "npm run clean && npm run create-dirs && npm run copy-static && npm run copy-deps",
    "prepare": "npm run build",
    "update-deps": "npm update --save && npm update --save-dev && npm run build",
    "start": "python3 -m http.server 5500"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faddat/k50.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/faddat/k50/issues"
  },
  "homepage": "https://github.com/faddat/k50#readme",
  "type": "module"
}
